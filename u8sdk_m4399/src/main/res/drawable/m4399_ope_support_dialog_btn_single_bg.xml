            <corners android:bottomLeftRadius="@dimen/m4399_dialog_radius" android:bottomRightRadius="@dimen/m4399_dialog_radius" />
            <solid android:color="@color/m4399_color_ripple_grey" />
        </shape>
    </item>
</selector>
